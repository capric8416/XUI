    <ResolutionTitle type="class">
        <position x="0" y="100" width="100" height="100"></position>
        <text>
            <normal>Resolution</normal>
        </text>
    </ResolutionTitle>

    <Resolution type="class">
        <position x="100" y="100" width="200" height="100"></position>
