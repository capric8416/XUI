    <Resolution_720p type="class">
        <position x="300" y="100" width="100" height="100"></position>
        <text>
            <normal>720p</normal>
        </text>
    </Resolution_720p>

    <Switch type="class">
        <background>
            <checked color="0x009688"></checked>
        </background>
        <border>
            <normal color="0x009688"></normal>
        </border>
        <text>
            <normal color="0x009688"></normal>
            <checked color="0x00000"></checked>
        </text>
