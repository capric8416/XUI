<Style>
    <Body type="id">
        <padding left="-5" top="-5" right="-5" bottom="-5"></padding>
        <background>
            <normal color="0xffffff"></normal>
        </background>
        <text>
            <normal h-align="center" v-align="center" color="0x000000"></normal>
        </text>
    </Body>

    <RadioGroup type="tag">
        <padding left="-1" top="-1" right="-1" bottom="-1"></padding>
        <border>
            <normal color="0x009688"></normal>
        </border>
    </RadioGroup>

    <RadioBox type="tag">
        <background>
            <checked color="0x009688"></checked>
        </background>
        <border>
            <normal color="0x009688"></normal>
        </border>
        <text>
            <normal color="0x009688"></normal>
            <checked color="0xffffff"></checked>
        </text>
    </RadioBox>

    <LeftNavTab type="id">
        <position x="0" y="0" width="150" height="500"></position>
    </LeftNavTab>

    <CastScreen type="id">
        <position x="0" y="0" width="1000" height="200"></position>
        <text>
            <normal>CastScreen</normal>
        </text>
    </CastScreen>

    <CastWindow type="id">
        <position x="0" y="200" width="1000" height="200"></position>
        <text>
            <normal>CastWindow</normal>
        </text>
    </CastWindow>

    <CastVideo type="id">
        <position x="0" y="400" width="1000" height="200"></position>
        <text>
            <normal>CastVideo</normal>
        </text>
    </CastVideo>

    <CastAudio type="id">
        <position x="0" y="600" width="1000" height="200"></position>
        <text>
            <normal>CastAudio</normal>
        </text>
    </CastAudio>

    <CastImage type="id">
        <position x="0" y="800" width="1000" height="200"></position>
        <text>
            <normal>CastImage</normal>
        </text>
    </CastImage>

    <CenterSettings type="id">
        <position x="150" y="0" width="550" height="800"></position>
        <padding left="50"></padding>
    </CenterSettings>

    <DisplayTitle type="id">
        <position x="0" y="0" width="100" height="100"></position>
    </DisplayTitle>

    <Display type="id">
        <position x="100" y="0" width="200" height="100"></position>
    </Display>

    <DefaultDisplay type="id">
        <position x="200" y="0" width="100" height="100"></position>
        <text>
            <normal>DefaultDisplay</normal>
        </text>
    </DefaultDisplay>

    <VirtualDisplay type="id">
        <position x="300" y="0" width="100" height="100"></position>
        <text>
            <normal>VirtualDisplay</normal>
        </text>
